<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="assets/css/styles.css" />
    <link rel="stylesheet" href="assets/css/checkboxes.css" />
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <title>Document</title>
</head>
<body>
    <%- include('./partials/non-user-header'); %> 
    <div class="divider">
        <section id="filters">
            <h3>Filters</h3>
            <form id="filters" action="#" class="form-group">
                <h4>Platform:</h4>
                <input type="checkbox" class="css-checkbox" id="platform_steam" name="platform_steam" />&nbsp;<label class="css-label lite-gray-check" for="platform_steam">Steam</label><br>
                <input type="checkbox" class="css-checkbox" id="platform_amazon" name="platform_amazon" />&nbsp;<label class="css-label lite-gray-check" for="platform_amazon">Amazon</label><br>
                <input type="checkbox" class="css-checkbox" id="platform_epic" name="platform_epic" />&nbsp;<label class="css-label lite-gray-check" for="platform_epic">Epic Games</label><br>
                <input type="checkbox" class="css-checkbox" id="platform_origin" name="platform_origin" />&nbsp;<label class="css-label lite-gray-check" for="platform_origin">Origin</label><br>
                <input type="checkbox" class="css-checkbox" id="platform_gamestop" name="platform_gamestop" />&nbsp;<label class="css-label lite-gray-check" for="platform_gamestop">GameStop</label><br>
                <input type="checkbox" class="css-checkbox" id="platform_uplay" name="platform_uplay" />&nbsp;<label class="css-label lite-gray-check" for="platform_uplay">Uplay</label><br>
                <h4>Currency:</h4>
                <input type="checkbox" class="css-checkbox" id="currency_cad" name="currency_cad" />&nbsp;<label class="css-label lite-gray-check" for="currency_cad">CAD</label><br>
                <input type="checkbox" class="css-checkbox" id="currency_usd" name="currency_usd" />&nbsp;<label class="css-label lite-gray-check" for="currency_usd">USD</label><br>
                <input type="checkbox" class="css-checkbox" id="currency_eur" name="currency_eur" />&nbsp;<label class="css-label lite-gray-check" for="currency_eur">EUR</label><br>
                <input type="checkbox" class="css-checkbox" id="currency_gbp" name="currency_gbp" />&nbsp;<label class="css-label lite-gray-check" for="currency_gbp">GBP</label><br>
            </form>
            <input type="submit" class="btn btn-dark" form="filters" value="Use Filters" />
        </section>

        <section id="results">
            <h1>Results for: "<%- searchName %>"</h1>
        <% items.forEach(function(item) { %>

            <div class="card">
                <div class="card-horizontal">
                    <div class="card-body">
                        <table>
                            <tbody>
                                <tr>
                                    <th>Game</th>
                                    <td><%= item.title %></td>
                                </tr>
                                <tr>
                                    <th>Regular Price</th>
                                    <td><%= item.normalPrice %></td>
                                </tr>
                                <tr>
                                    <th>Sale Price</th>
                                    <td><%= item.salePrice %></td>
                                </tr>
                                <tr>
                                    <th>Platform</th>
                                    <% if(item.storeID == 1){ %>
                                        <td>Steam</td>
                                    <% } %>
                                    <% if(item.storeID == 4){ %>
                                        <td>Amazon</td>
                                    <% } %>
                                    <% if(item.storeID == 5){ %>
                                        <td>GameStop</td>
                                    <% } %>
                                    <% if(item.storeID == 8){ %>
                                        <td>Origin</td>
                                    <% } %>
                                    <% if(item.storeID == 13){ %>
                                        <td>Uplay</td>
                                    <% } %>
                                    <% if(item.storeID == 25){ %>
                                        <td>Epic Games</td>
                                    <% } %>
                                </tr>
                                <tr>
                                    <th>Buy Now</th>
                                    <td><a href="https://www.cheapshark.com/redirect?dealID={<%= item.dealID %>}" >Link</a></td>
                                </tr>
                                <tr>
                                    <th>Add to Likes</th>
                                    <td>Like</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="img-square-wrapper"><img class="card-img-right" src="<%= item.thumb %>" /></div>
                </div>
            </div>
            <% }); %>
        </section>
      </div>
</body>
</html>