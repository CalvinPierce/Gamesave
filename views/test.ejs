<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div>
        <% items.forEach(function(item) { %>
          <ul>
            <li>Game: <%= item.title %></li> 
            <li>Regular Price: <%= item.normalPrice %></li>
            <li>Sale Price: <%= item.salePrice %></li>
            <li><%= item.storeID %></li>
            <% if(item.storeID == 1){ %>
                <li>Retailer: Steam</li>
                <% } %>
            <li><img src="<%= item.thumb %>"></li>
            <li>Buy Now: <a href="https://www.cheapshark.com/redirect?dealID={<%= item.dealID %>}" +>Link</a></li>
          </ul>
        <% }); %>
      </div>
</body>
</html>